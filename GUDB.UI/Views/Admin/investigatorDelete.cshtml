
@model GUDB.Model.Investigator

@{
    ViewBag.Title = "删除";
    Layout = null;
}



@*<h2>UserLook</h2>*@


<script src="~/Scripts/jquery-3.4.1.js"></script>
<script src="~/Scripts/bootstrap.js"></script>

@Styles.Render("~/bundles/Bootstarp/css")
@Styles.Render("~/bundles/Bootstarp/css")


<script>

    $(function () {
        $("#i1").hide();
        $("#i2").hide();
        $("#i3").hide();
        $("#i4").hide();
        $("#i5").hide();
        $("#i6").hide();

        $("#i8").hide();
        $("#i7").hide();

    }
    );
</script>

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    <div>
        <div class="alert alert-warning alert-dismissable">
            <button type="button" class="close" data-dismiss="alert"
                    aria-hidden="true">
                &times;
            </button>
            你正在删除 调查人员 @Html.DisplayFor(model => model.IName) 信息
        </div>



        <hr />
        <table class="table table-hover">

            <tbody>


                <!--UID-->
                <tr>
                    <td>
                        身份证号

                    </td>



                    <td>

                        @Html.DisplayFor(model => model.IId)
                    </td>


                    @*<td><a href="~/Admin/Change?key=@Html.DisplayFor(model => model.UId) && type=UId ">修改信息</a></td>*@





                    <td>


                        <div class="col-lg-4">

                            @Html.EditorFor(model => model.IId, new { htmlAttributes = new { @class = "form-control", id = "i1" } })


                        </div>
                    </td>

                </tr>



                <!--UName-->
                <tr>
                    <td>

                      姓名
                    </td>

                    <td>

                        @Html.DisplayFor(model => model.IName)
                    </td>



                    @*<td><a href="~/Admin/Change?key=@Html.DisplayFor(model => model.UId) && type=UName ">修改信息</a></td>*@






                    <td>


                        <div class="col-lg-4">

                            @Html.EditorFor(model => model.IName, new { htmlAttributes = new { @class = "form-control", @id = "i2" } })



                        </div>
                    </td>
                </tr>



                <!--UP  a  s-->

                <tr>
                    <td>
                      电话

                    </td>

                    <td>

                        @Html.DisplayFor(model => model.IPhone)
                    </td>






                    <td>


                        <div class="col-lg-4">

                            @Html.EditorFor(model => model.IPhone, new { htmlAttributes = new { @class = "form-control", @id = "i8" } })



                        </div>
                    </td>

                    @*<td><a href="~/Admin/Change?key=@Html.DisplayFor(model => model.UId) && type=UPhone ">修改信息</a></td>*@

                </tr>



                <!--UPhone-->

                <tr>
                    <td>
                        邮箱

                    </td>

                    <td>

                        @Html.DisplayFor(model => model.IEmail)
                    </td>






                    <td>


                        <div class="col-lg-4">

                            @Html.EditorFor(model => model.IEmail, new { htmlAttributes = new { @class = "form-control", @id = "i3" } })



                        </div>
                    </td>

                    @*<td><a href="~/Admin/Change?key=@Html.DisplayFor(model => model.UId) && type=UPhone ">修改信息</a></td>*@

                </tr>




                <!--邮 箱-->
                <tr>
                    <td>
                        单位
                    </td>

                    <td>

                        @Html.DisplayFor(model => model.Unit.UName)
                    </td>






                   

                    @*<td><a href="~/Admin/Change?key=@Html.DisplayFor(model => model.UId) && type=UMail ">修改信息</a></td>*@

                </tr>





            </tbody>
        </table>
    </div>

    <br />



    <div class="form-actions no-color">
        @Html.ActionLink("返回列表", "investigatorList")
        |



        <input type="submit" value="删除" class="btn btn-default" />

    </div>


}

