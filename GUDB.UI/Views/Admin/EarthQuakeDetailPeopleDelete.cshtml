
@model GUDB.Model.DamagePeople

@{
    ViewBag.Title = "删除";
    Layout = null;
}



@*<h2>UserLook</h2>*@


<script src="~/Scripts/jquery-3.4.1.js"></script>
<script src="~/Scripts/bootstrap.js"></script>

@Styles.Render("~/bundles/Bootstarp/css")
@Styles.Render("~/bundles/Bootstarp/css")


<script>

    $(function () {
        $("#i1").hide();
        $("#i2").hide();
        $("#i3").hide();
        $("#i4").hide();
        $("#i5").hide();
        $("#i6").hide();

        $("#i8").hide(); $("#i10").hide();
        $("#i7").hide();          $("#i9").hide();

    }
    );
</script>



@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    <div>
        <div class="alert alert-warning alert-dismissable">
            <button type="button" class="close" data-dismiss="alert"
                    aria-hidden="true">
                &times;
            </button>
            你正在 删除伤亡人员 @Html.DisplayFor(model => model.DPName) 信息
        </div>



        <hr />
        <table class="table table-hover">

            <tbody>


                <!--UID-->
                <tr>
                    <td>
                        身份证号码

                    </td>



                    <td>

                        @Html.DisplayFor(model => model.DPId)
                    </td>


                    @*<td><a href="~/Admin/Change?key=@Html.DisplayFor(model => model.UId) && type=UId ">修改信息</a></td>*@



                    <td>


                        <div class="col-lg-4">


                            @Html.EditorFor(model => model.DPId, new { htmlAttributes = new { @class = "form-control", id = "i1" } })


                        </div>
                    </td>

                </tr>



                <!--UName-->
                <tr>
                    <td>
                        姓名

                    </td>

                    <td>

                        @Html.DisplayFor(model => model.DPName)
                    </td>



                    <td>


                        <div class="col-lg-4">

                            @Html.EditorFor(model => model.DPName, new { htmlAttributes = new { @class = "form-control", @id = "i2" } })



                        </div>
                    </td>
                </tr>



                <!--UP  a  s-->

                <tr>
                    <td>
                        电话

                    </td>

                    <td>

                        @Html.DisplayFor(model => model.DPPhone)
                    </td>



                    <td>


                        <div class="col-lg-4">

                            @Html.EditorFor(model => model.DPPhone, new { htmlAttributes = new { @class = "form-control", @id = "i8" } })



                        </div>
                    </td>

                    @*<td><a href="~/Admin/Change?key=@Html.DisplayFor(model => model.UId) && type=UPhone ">修改信息</a></td>*@

                </tr>



                <!--UPhone-->

                <tr>
                    <td>
                        电邮

                    </td>

                    <td>

                        @Html.DisplayFor(model => model.DPEmail)
                    </td>





                    <td>


                        <div class="col-lg-4">

                            @Html.EditorFor(model => model.DPEmail, new { htmlAttributes = new { @class = "form-control", @id = "i3" } })



                        </div>
                    </td>


                </tr>




                <!--邮 箱-->
                <tr>
                    <td>
                        其他联系方式
                    </td>

                    <td>

                        @Html.DisplayFor(model => model.DPOtherConnect)
                    </td>




                    <td>


                        <div class="col-lg-4">

                            @Html.EditorFor(model => model.DPEmail, new { htmlAttributes = new { @class = "form-control", @id = "i4" } })



                        </div>
                    </td>



                </tr>
                @Html.EditorFor(model => model.IId, new { htmlAttributes = new { @class = "form-control", id = "i10" } })



                @Html.EditorFor(model => model.EId, new { htmlAttributes = new { @class = "form-control", id = "i5" } })

                <tr>
                    <td>
                        所受伤害程度
                    </td>

                    <td>

                        @Html.DisplayFor(model => model.DPLevel)
                    </td>




                    <td>


                        <div class="col-lg-4">

                            @Html.EditorFor(model => model.DPLevel, new { htmlAttributes = new { @class = "form-control", @id = "i6" } })



                        </div>
                    </td>

                    @*<td><a href="~/Admin/Change?key=@Html.DisplayFor(model => model.UId) && type=UMail ">修改信息</a></td>*@

                </tr>


                <tr>
                    <td>
                        关联联系人姓名
                    </td>

                    <td>

                        @Html.DisplayFor(model => model.DPReferPeopleName)
                    </td>




                    <td>


                        <div class="col-lg-4">

                            @Html.EditorFor(model => model.DPReferPeopleName, new { htmlAttributes = new { @class = "form-control", @id = "i9" } })



                        </div>
                    </td>

                    @*<td><a href="~/Admin/Change?key=@Html.DisplayFor(model => model.UId) && type=UMail ">修改信息</a></td>*@

                </tr>



                <tr>
                    <td>
                        关联联系人电话
                    </td>

                    <td>

                        @Html.DisplayFor(model => model.DPReferPeoplePhone)
                    </td>




                    <td>


                        <div class="col-lg-4">

                            @Html.EditorFor(model => model.DPReferPeoplePhone, new { htmlAttributes = new { @class = "form-control", @id = "i7" } })



                        </div>
                    </td>

                    @*<td><a href="~/Admin/Change?key=@Html.DisplayFor(model => model.UId) && type=UMail ">修改信息</a></td>*@

                </tr>







            </tbody>
        </table>
    </div>

    <br />



    <div class="form-actions no-color">
        |
        @Html.ActionLink("返回列表", "EarthQuakeDetailPeopleList", new { id = Model.EId })
        |

        <input type="submit" value="删除" class="btn btn-default" />

    </div>


}


