        function B2() {
            $("#showMain").empty();
            $html = '<br/> <br/><div class="theme-default-content content__default"> <h1>接口格式规范</h1 >' +
                '<h3 id="请求格式说明"><a href="#请求格式说明" aria-hidden="true" class="header-anchor">#</a> 请求格式说明</h3> '+
